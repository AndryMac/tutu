<%= form_for [:admin, @railway_station] do |f| %>
    <%= render 'common/errors', resource: @railway_station %>

  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title, class: "form-control" %>
  </div>

  <div class="actions">
    <%= f.submit t('.submit', action: @railway_station.new_record? ? 'Добавить' : 'Редактировать' ), class: 'btn btn-success', type: "button"%>
  </div>
<% end %>
